{
  "name": "micro-python-terminal",
  "displayName": "Âµ-python-terminal",
  "description": "A terminal for MicroPython development.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.47.0"
  },
  "publisher": "ladvien",
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "micro-python-terminal.sendTextTermCommand",
        "title": "MicroPython: Send to REPL"
      },
      {
        "command": "micro-python-terminal.createTerm",
        "title": "MicroPython: Create MicroPython Terminal"
      },
      {
        "command": "micro-python-terminal.selectDevice",
        "title": "MicroPython: Select MicroPython USB device"
      },
      {
        "command": "micro-python-terminal.clearLogs",
        "title": "MicroPython: Clear Logs"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "lint": "eslint src --ext ts",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.1",
    "@types/mocha": "^7.0.2",
    "@types/node": "^13.11.0",
    "@types/serialport": "^8.0.1",
    "@types/vscode": "^1.47.0",
    "@typescript-eslint/eslint-plugin": "^2.30.0",
    "@typescript-eslint/parser": "^2.30.0",
    "chai": "^4.2.0",
    "electron-rebuild": "^1.11.0",
    "eslint": "^6.8.0",
    "glob": "^7.1.6",
    "mocha": "^7.2.0",
    "mocha-logger": "^1.0.6",
    "typescript": "^3.8.3",
    "vscode-test": "^1.3.0"
  },
  "dependencies": {
    "@serialport/bindings": "^9.0.1",
    "@types/serialport": "^8.0.1",
    "chai": "^4.2.0",
    "g": "^2.0.1",
    "save": "^2.4.0",
    "serialport": "^9.0.1"
  }
}
